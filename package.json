{
  "name": "boom-box",
  "version": "0.0.1",
  "description": "play dats which have mp3s in them",
  "main": "index.js",
  "scripts": {
    "build": "run-p build:yo",
    "build:yo": "browserify -p tinyify lib/src/yo-yo.js > lib/yo-yo.js"
  },
  "browserify": {
    "transform": [
      "babelify"
    ],
    "plugin": [
      "tinyify"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "yo-yo": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "npm-run-all": "^4.1.5",
    "tinyify": "^2.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mixmix/boom-box.git"
  },
  "keywords": [
    "DAT",
    "music",
    "player"
  ],
  "author": "mixmix",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/mixmix/boom-box/issues"
  },
  "homepage": "https://github.com/mixmix/boom-box#readme"
}
